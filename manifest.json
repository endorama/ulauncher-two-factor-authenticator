{
  "required_api_version": "^2.0.0",
  "name": "Two Factor Authenticator extension",
  "description": "Secure generation of 2FA tokens",
  "developer_name": "Edoardo Tenani",
  "icon": "images/two-factor-authenticator.png",
  "options": {
    "query_debounce": 0.1
  },
  "preferences": [
    {
      "id": "2fa_gen",
      "type": "keyword",
      "name": "2fa",
      "description": "Generate 2FA token",
      "default_value": "2fa"
    },
    {
      "id": "2fa_fuzzy_threshold",
      "type": "input",
      "name": "fuzzy threshold",
      "description": "Threshold used in filtering fuzzy matching value. Greater the value, more precise should be the match to be displayed. Very high value can hide almost all entries.",
      "default_value": 40
    }
  ]
}
